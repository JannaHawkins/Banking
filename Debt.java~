class Debt{
    private String name;
    private int debtTotal;
    private int interestRate;
    private int deadline;

    Debt(String name, int debtTotal, int interestRate){
	this.name = name;
	this.debtTotal = debtTotal;
	this.interestRate = interestRate;
    }

    int getDebtTotal(){
	return debtTotal;
    }

    
    
}
